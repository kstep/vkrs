
#[derive(Debug, PartialEq, Eq, Deserialize, Clone, Copy)]
pub struct Counters {
    #[serde(default)]
    pub friends: usize,
    #[serde(default)]
    pub messages: usize,
    #[serde(default)]
    pub photos: usize,
    #[serde(default)]
    pub videos: usize,
    #[serde(default)]
    pub notes: usize,
    #[serde(default)]
    pub gifts: usize,
    #[serde(default)]
    pub events: usize,
    #[serde(default)]
    pub groups: usize,
    #[serde(default)]
    pub sdk: usize,
}

#[derive(Debug, PartialEq, Eq, Deserialize, Clone)]
pub struct Contacts {
    pub found: Vec<User>,
    pub other: Vec<Contact>,
}

#[derive(Debug, PartialEq, Eq, Deserialize, Clone)]
pub struct Contact {
    pub contact: String,
    pub common_count: usize,
}

#[derive(Debug, PartialEq, Eq, Deserialize, Clone)]
pub struct PushSettings {
    pub disabled: Bool,
    pub disabled_until: Timestamp,
    pub conversations: u8, // TODO: actual structure unknown from docs
}


#[derive(Debug, PartialEq, Eq, Deserialize, Clone)]
pub struct Offer {
    pub id: String,
    pub title: String,
    pub instruction: String,
    pub instruction_html: String,
    pub short_description: String,
    pub description: String,
    pub img: String,
    pub tag: String,
    pub price: u32,
}

#[derive(Debug, PartialEq, Eq, Deserialize, Clone)]
pub struct Info {
    pub country: String,
    pub https_required: Bool,
    pub own_posts_default: Bool,
    pub no_wall_replies: Bool,
    pub intro: u32, // TODO: limits unknown
    pub lang: u16,
}
